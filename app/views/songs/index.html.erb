<%= form_for @song do |f| %>
<p>
  <%= f.label :title %>
  <%= f.text_field :title %>
</p>
  <p>
    <%= f.label :artist %>
    <%= f.text_field :artist %>
  </p>
  <p>
    <%= f.submit 'Add song' %>
  </p>
<% end %>

<% @songs.each do |song| %>
<ul>
  <li>
    <%= link_to "#{song.title}", song_path(song) %>, <%= song.artist %>
  </li>
</ul>
<% end %>